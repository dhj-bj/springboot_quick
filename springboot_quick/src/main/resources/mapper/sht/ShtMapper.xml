<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dhj.demo.mapper.sht.ShtMapper">
    <select id="getShtUser" resultType="com.dhj.demo.pojo.sht.UserSht" >
        select * from a_atest
    </select>
    <select id="getUserListById" resultType="com.dhj.demo.pojo.sht.UserSht" >
        select * from a_atest where id = #{id}
    </select>
</mapper>